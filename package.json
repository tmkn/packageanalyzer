{
  "name": "@tmkn/packageanalyzer",
  "version": "0.9.11",
  "description": "A tool to analyze a Node.js project/package",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "exports": {
    "./web": "./build/src/index.web.js"
  },
  "typesVersions": {
    "*": {
      "web": [
        "./build/src/index.web.d.ts"
      ]
    }
  },
  "files": [
    "build/src"
  ],
  "bin": {
    "pkga": "./build/src/cli.js"
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:clean": "node build/tests/clean.js",
    "prettier": "prettier --write \"{src,tests,utility}/**/{*.ts,*.js}\" webpack.web.config.js",
    "pa": "node build/src/cli.js",
    "preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "lint": "eslint \"src/**\" \"tests/**\"",
    "lint:fix": "eslint \"src/**\" \"tests/**\" --fix",
    "lint:web": "node ./webpack.web.config.js",
    "prepublishOnly": "npm run build",
    "publish:test": "npm publish --dry-run"
  },
  "author": "tmkn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmkn/packageanalyzer.git"
  },
  "keywords": [
    "package",
    "analyzer",
    "dependency",
    "tree"
  ],
  "bugs": {
    "url": "https://github.com/tmkn/packageanalyzer/issues"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^17.0.0",
    "@types/semver": "^7.3.4",
    "@types/spdx-satisfies": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "memory-fs": "^0.5.0",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.7.2",
    "webpack": "^5.65.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "clipanion": "^3.2.0-rc.4",
    "dayjs": "^1.9.6",
    "fp-ts": "^2.11.6",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.0",
    "lodash": "^4.17.20",
    "ora": "^5.1.0",
    "semver": "^7.3.2",
    "spdx-satisfies": "^5.0.0"
  },
  "engines": {
    "node": ">=14.16.1"
  }
}
