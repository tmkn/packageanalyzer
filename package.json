{
  "name": "packageanalyzer",
  "version": "0.9.0-beta.0",
  "description": "A tool to analyze a Node.js project/package",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "files": [
    "/build/src/**/*.js",
    "/build/src/**/*.d.ts"
  ],
  "bin": {
    "npa": "./build/src/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:clean": "node build/tests/clean.js",
    "prettier": "prettier --write \"{src,tests,utility}/**/{*.ts,*.js}\"",
    "cli": "node build/src/cli.js",
    "preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "lint": "eslint \"src/**\" \"tests/**\"",
    "lint:fix": "eslint \"src/**\" \"tests/**\" --fix",
    "package:test": "npm publish --dry-run"
  },
  "author": "tmkn",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.0",
    "@types/semver": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "dayjs": "^1.8.18",
    "ora": "^4.0.3",
    "semver": "^7.1.1"
  }
}
