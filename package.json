{
    "name": "@tmkn/packageanalyzer",
    "version": "0.9.14",
    "description": "A tool to analyze a Node.js project/package",
    "main": "./build/src/index.js",
    "types": "./build/src/index.d.ts",
    "exports": {
        ".": "./build/src/index.js",
        "./web": "./build/src/index.web.js"
    },
    "typesVersions": {
        "*": {
            "web": [
                "./build/src/index.web.d.ts"
            ]
        }
    },
    "files": [
        "build/src"
    ],
    "bin": {
        "pkga": "./build/src/cli.js"
    },
    "scripts": {
        "build": "tsc -b",
        "dev": "tsc -b --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:update": "jest --updateSnapshot",
        "test:clean": "node build/tests/clean.js",
        "test:exports": "node ./build/src/cli.js lint --package react ./tests/exportTest.js",
        "prettier": "prettier --write \"{src,tests,utility}/**/{*.ts,*.js}\" webpack.web.config.js",
        "pa": "node build/src/cli.js",
        "preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
        "lint": "eslint \"src/**\" \"tests/**\"",
        "lint:fix": "eslint \"src/**\" \"tests/**\" --fix",
        "lint:web": "node ./webpack.web.config.js",
        "prepublishOnly": "npm run build",
        "publish:test": "npm publish --dry-run"
    },
    "author": "tmkn",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tmkn/packageanalyzer.git"
    },
    "keywords": [
        "package",
        "analyzer",
        "dependency",
        "tree",
        "lint"
    ],
    "bugs": {
        "url": "https://github.com/tmkn/packageanalyzer/issues"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/lodash": "^4.14.202",
        "@types/node": "^22.1.0",
        "@types/semver": "^7.5.8",
        "@types/spdx-satisfies": "^0.1.2",
        "@types/tar": "^6.1.10",
        "eslint": "9.17.0",
        "express": "^4.18.2",
        "globals": "^15.9.0",
        "jest": "^29.7.0",
        "memory-fs": "^0.5.0",
        "nock": "^13.4.0",
        "prettier": "^3.1.1",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.1",
        "typescript": "^5.6.2",
        "typescript-eslint": "^8.8.0",
        "webpack": "^5.89.0"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "clipanion": "^4.0.0-rc.2",
        "dayjs": "^1.11.10",
        "lodash": "^4.17.20",
        "ora": "^5.1.0",
        "postinstall-postinstall": "^2.1.0",
        "semver": "^7.6.0",
        "spdx-satisfies": "^5.0.1",
        "tar": "^7.4.3",
        "zod": "^3.22.4"
    },
    "engines": {
        "node": ">=18"
    },
    "resolutions": {
        "cliui/wrap-ansi": "7.0.0"
    }
}
